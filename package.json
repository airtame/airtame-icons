{
  "name": "airtame-icons",
  "version": "0.0.0-alpha.1",
  "description": "Set of Icons used by Airtame available as SVGs and React Components",
  "main": "index.js",
  "bin": "cli.js",
  "homepage": "http://airtame.github.io/airtame-icons",
  "repository": {
    "type": "git",
    "url": "https://github.com/airtame/airtame-icons.git"
  },
  "license": "ISC",
  "author": "Airtame",
  "contributors": [
    "Marcos Mellado <marcos@airtame.com> (http://mellados.com)"
  ],
  "keywords": [
    "airtame",
    "icons",
    "svg",
    "react"
  ],
  "dependencies": {
    "argparse": "^1.0.10",
    "fs-extra": "^5.0.0",
    "ncp": "^2.0.0",
    "util.promisify": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "devDependencies": {
    "airtame-gooey": "^2.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airtame": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.20.2",
    "svg-sprite": "^1.3.7",
    "svgo": "^1.0.4",
    "svgr": "^1.8.1",
    "svgstore": "^2.0.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "scripts": {
    "clean": "rimraf -rf build && rimraf icon-map.js && rimraf -rf dist && rimraf -rf release",
    "build:icons": "node ./src/build.js",
    "build:files": "copyfiles -f ./src/cli.js README.md CHANGELOG.md package.json release",
    "build:site": "cross-env NODE_ENV=production webpack",
    "build:babel": "cross-env NODE_ENV=release babel ./build --out-dir ./release --ignore *.spec.js,test,node_modules",
    "build": "npm run clean && npm run build:icons && npm run build:babel && npm run build:files && npm run build:site",
    "dev": "npm run clean && npm run build:icons && cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "gh-pages": "gh-pages -d dist",
    "publish": "npm publish ./release",
    "release": "npm run build && npm run publish && npm run gh-pages"
  }
}
